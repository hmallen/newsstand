# Copy to .env and customize values

# Choose ONE mode:
# 1) Incoming Webhook
SLACK_WEBHOOK_URL=
# 2) Bot token
SLACK_BOT_TOKEN=
SLACK_CHANNEL_ID=

USE_SLACK_WEBHOOK=true
USE_GENERIC_WEBHOOK=false
GENERIC_WEBHOOK_URL=

# Optional presentation
SLACK_USERNAME=news-bot
SLACK_ICON_EMOJI=:police_car:

# Polling & storage
POLL_INTERVAL=300
SEEN_FILE=seen.json

# Required: path to JSON config created in this repo
CONFIG_FILE=./config.json
